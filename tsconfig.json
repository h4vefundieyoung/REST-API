{
  "compilerOptions": {
  "target": "ESNext",
  "module": "ESNext",
  "rootDir": "./src",
  "outDir": "./dist",
  "esModuleInterop": true,
  "moduleResolution": "node",
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true
},
 "include": ["src/**/*.ts"],
 "exclude": ["node_modules"]
}